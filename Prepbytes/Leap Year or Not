#include <bits/stdc++.h>
using namespace std;

int main()
{
  //write your code here
  int t,year;
  
  cin>>t;
  
  for(int i=0;i<t;i++)
  {
  cin>>year;
  if(year%400==0)
  {
    printf("Yes\n");
    printf("%d",year%4);
    printf("Year=%d",year);
  
    break;
  }
  else if(year%100!=0 && year%4==0)
  {
    printf("Yes");
  }
  else
  {printf("No\n");}
  }
  return 0;
}
