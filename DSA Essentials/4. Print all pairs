//Print all sub arrays
#include <iostream>
using namespace std;

void printsubarrays(int arr[], int size){
    for(int i=0; i<size; i++){
        for(int j=i; j<size; j++){
            cout<<endl<<i<<","<<j;
        }
    }
}

int main() {
	// your code goes here
    int size;
    cin>>size;

    int arr[size]={0};

    for(int i=0;i<size; i++){
        cin>>arr[i];
    }
    printsubarrays(arr,size);
	return 0;
}

